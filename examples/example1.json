{
	"HTML": {
		"head": {
			"$ref": "head1.json"
		},
		"$style": {
			"$ref": "style1.json"
		},
		"body": {
			"div": {
				"$attr.class": "row center-text",
				"h1": "Guest List"
			},
			"br$no-open": "",
			"$view:Controller1@controller1.js": {
				"table": {
					"$attr.class": "table table-striped table-bordered",
					"tr#1": {
						"td#1": {
							"b": "Name"
						},
						"td#2": {
							"b": "Surname"
						}
					},
					"tr#2": {
						"td#1": "Stephan",
						"td#2": "Coertzen"
					}
				},
				"button": {
					"$attr.class": "btn btn-default",
					"$attr.onclick": "$.AddUser()",
					"span":"Add"
				}
			}
		}
	}
}
